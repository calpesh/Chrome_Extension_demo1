{
  "manifest_version": 2,
	"name": "UA Demo Extension",
	"version": "1.0",
	"description": "UA Demo Extension",
	"content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
	
  "browser_action":{
    "default_icon":"scraper.png",
    "default_popup":"popup.html"
  }
}